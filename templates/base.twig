<!doctype html>
<!--[if lt IE 9]><html class="no-js no-svg ie lt-ie9 lt-ie8 lt-ie7" {{ site.language_attributes }}> <![endif]-->
<!--[if IE 9]><html class="no-js no-svg ie ie9 lt-ie9 lt-ie8" {{ site.language_attributes }}> <![endif]-->
<!--[if gt IE 9]><!-->
<!--<![endif]-->
<html class="no-js no-svg" {{ site.language_attributes }}>
    {% block html_head_container %}

    {% include 'html-header.twig' %}
    {% block head %}
    {% endblock %}
    {% endblock %}

    <body class="{{body_class}}" data-template="base.twig">
        <a class="skip-link screen-reader-text" href="#content">{{ _e( 'Skip to content') }}</a>

        <header class="header">
            {% block header %}
            {{ react_component('Nav', { 'props': { site: site, menu: menu } }) }}
            {% endblock %}
        </header>

        <section id="content" role="main" class="content-wrapper">
            {# {{ react_component_array('NavBar') }} #}
            {% if sidebar %}
            <aside class="layout-sidebar">
                {# {{ react_component_array('NavBar') }} #}
            </aside>
            {% endif %}
        </section>

        {% block footer %}
        <footer id="footer">
            {% include 'footer.twig' %}
        </footer>
        {{ function('wp_footer') }}
        {% endblock %}
    </body>

</html>
